    <div class="navbar-header">
        <button type="button" data-target="#navbarCollapse" data-toggle="collapse"
                class="navbar-toggle">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
        <div class="navbar-brand">
            <a href="/">
                <div id="logo-title"><img src="images/logo.png"/></div>
                <div id="logo-subtitle">Machine Learning et al.</div>
            </a>
        </div>
    </div>
    <!-- Collection of nav links and other content for toggling -->
    <div id="navbarCollapse" class="collapse navbar-collapse">
        <ul class="nav navbar-nav">
            <li>
                <a ng-class="nav.getClass('blog')" href="/"><i class="fa fa-home"></i>Blog</a>
            </li>
            <li>
                <a ng-class="nav.getClass('neural-network')"
                   href="category/neural-network/"><i class="fa fa-sitemap"></i>
                    <div>Neural
                        Network from Scratch
                    </div>
                </a>
            </li>
            <li>
                <a ng-class="nav.getClass('about')" href="about/"><i class="fa fa-user"></i>About</a>
            </li>
            <li ng-show="main.isLoggedIn()" class="ng-cloak">
                <a ng-class="nav.getClass('lab')" href="lab/"><i class="fa fa-flask"></i>Lab</a>
            </li>
            <li ng-show="main.isLoggedIn()" class="ng-cloak">
                <a ng-class="nav.getClass('add')" href="add/"><i class="fa fa-plus"></i>Add Post</a>
            </li>
            <li ng-show="main.isLoggedIn()" class="ng-cloak">
                <a ng-class="nav.getClass('addCategory')" href="add-category/"><i class="fa
                    fa-plus"></i>Add Category</a>
            </li>
            <li ng-show="main.isLoggedIn()" class="ng-cloak">
                <a href="" ng-click="nav.logout()"><i class="fa fa-sign-out"></i>Logout</a>
            </li>
        </ul>
    </div>
</nav>

